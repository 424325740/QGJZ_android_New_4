<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:orientation="vertical"
    android:layout_marginLeft="@dimen/DIMEN_120PX"
    android:layout_marginRight="@dimen/DIMEN_120PX">
    <LinearLayout
        android:layout_marginTop="@dimen/DIMEN_30PX"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <View
            android:layout_weight="1"
            android:layout_width="wrap_content"
            android:layout_height="14dp"/>
        <ImageView
            android:id="@+id/finsh"
            android:layout_marginRight="@dimen/DIMEN_30PX"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:padding="@dimen/DIMEN_20PX"
            android:src="@mipmap/btn_xx"
            android:layout_gravity="right"
            />
    </LinearLayout>
    <TextView
        android:id="@+id/isyueta"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="约TA成功"
        android:textColor="#fd6a6a"
        android:textSize="@dimen/DIMEN_36PX"
        android:gravity="center"/>
    <LinearLayout
        android:layout_marginTop="@dimen/DIMEN_60PX"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">
        <TextView
            android:id="@+id/jixu"
            android:layout_marginLeft="@dimen/DIMEN_20PX"
            android:layout_width="@dimen/DIMEN_160PX"
            android:layout_height="@dimen/DIMEN_64PX"
            android:text="继续约TA"
            android:gravity="center"
            android:background="@drawable/button_selected"
            android:textColor="#fd6a6a"
            />
        <TextView
            android:id="@+id/chakan"
            android:layout_marginLeft="@dimen/DIMEN_20PX"
            android:layout_width="@dimen/DIMEN_160PX"
            android:layout_height="@dimen/DIMEN_64PX"
            android:text="查看已约"
            android:gravity="center"
            android:background="@drawable/yuanjiaobut"
            android:textColor="#ffffff"
            />
    </LinearLayout>
    <View
        android:layout_width="match_parent"
        android:layout_height="@dimen/DIMEN_40PX"/>
</LinearLayout>
